<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="University_Management_System" transaction-type="RESOURCE_LOCAL">
		<class>org.jsp.app.model.dto.Application</class>
		<class>org.jsp.app.model.dto.ProgramsOffered</class>
		<class>org.jsp.app.model.dto.Participant</class>
		<class>org.jsp.app.model.dto.ProgramsScheduled</class>
		<class>org.jsp.app.model.dto.Users</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/university"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.password" value="12345"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
